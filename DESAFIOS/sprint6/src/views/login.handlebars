<main class="Login">
    <h1 class="Login-welcome">LOGIN</h1>
    <h3 class="Login-paragraph">Log in into your account!</h3>
    <form class="Login-form">
        <fieldset class="Login-form_box">
            <label for="email">Email</label>
            <input class="Login-form_box_input" type="text" id="login-email">
        </fieldset>
        <fieldset class="Login-form_box">
            <label for="password">Password</label>
            <input class="Login-form_box_input" type="password" id="login-password">
        </fieldset>
        <button
            id="login"
            type="button"
            class="Login-button"
        >LOGIN</button>
    </form>
</main>
<script src="/socket.io/socket.io.js"></script>
<script src="/public/scripts/login.js"></script>